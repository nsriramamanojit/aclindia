<% if @sampleconditions.blank? %>
 	<p>Oops!!! No Matches found.</p>
<% else %>
<table id="newspaper-b">
    <thead>
    	<tr>
        	<th scope="col">Sample Condition</th>
            <th scope="col">View</th>
            <th scope="col">Edit</th>
            <th scope="col">Delete</th>
 
        </tr>
    </thead>
         <tfoot>
    	<tr>
        	<td colspan="4"></td>
        </tr>
    </tfoot>
<% @sampleconditions.each do |samplecondition| %>
  <tr>
    <td><%= samplecondition.name %></td>
    <td><%= link_to 'View', samplecondition %></td>
    <td><%= link_to 'Edit', edit_samplecondition_path(samplecondition) %></td>
    <td><%= link_to 'Delete', samplecondition, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<% end %>
<%= link_to 'Back to Sample Conditions', samplecondition_path %>
