<h1>Search Results</h1>
<% if @labcentrals.blank? %>
 	<p>Oops!!! No Matches found.</p>
<% else %>
<table id="rounded-corner">
    <thead>
    	<tr>
        	<th scope="col" class="rounded-company">Lab Id</th>
            <th scope="col" class="rounded-q1">View</th>
            <th scope="col" class="rounded-q2">Edit</th>
            <th scope="col" class="rounded-q3">Delete</th>
 
        </tr>
    </thead>
         <tfoot>
    	<tr>
        	<td colspan="3" class="rounded-foot-left"></td>
        	<td class="rounded-foot-right">&nbsp;</td>
        </tr>
    </tfoot>
<% @labcentrals.each do |labcentral| %>
   <tr>
    <td><%= labcentral.labid %></td>
    <td><%= link_to 'View', labcentral %></td>
    <td><%= link_to 'Edit', edit_labcentral_path(labcentral) %></td>
    <td><%= link_to 'Delete', labcentral, :confirm => 'Are you sure?',
       :method => :delete %></td>
  </tr>
<% end %>
</table>
<% end %>
<%= link_to 'Back to Central Labs', labcentrals_path %>
