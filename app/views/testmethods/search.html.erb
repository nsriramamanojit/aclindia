<% if @testmethods.blank? %>
 	<p>Oops!!! No Matches found.</p>
<% else %>
<table id="newspaper-b">
    <thead>
    	<tr>
        	<th scope="col">Test Method</th>
            <th scope="col">View</th>
            <th scope="col">Edit</th>
            <th scope="col">Delete</th>
 
        </tr>
    </thead>
         <tfoot>
    	<tr>
        	<td colspan="4"></td>
        </tr>
    </tfoot>
<% @testmethods.each do |testmethod| %>
  <tr>
    <td><%= testmethod.name %></td>
    <td><%= link_to 'View', testmethod %></td>
    <td><%= link_to 'Edit', edit_testmethod_path(testmethod) %></td>
    <td><%= link_to 'Delete', testmethod, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<% end %>
<%= link_to 'Back to Methods', testmethods_path %>
