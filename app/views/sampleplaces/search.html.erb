<h1>Search Results</h1>
<% if @sampleplaces.blank? %>
 	<p>Oops!!! No Matches found.</p>
<% else %>
<table id="newspaper-b">
    <thead>
    	<tr>
        	<th scope="col">Sample Place</th>
            <th scope="col">View</th>
            <th scope="col">Edit</th>
            <th scope="col">Delete</th>
 
        </tr>
    </thead>
         <tfoot>
    	<tr>
        	<td colspan="4"></td>
        </tr>
    </tfoot>
<% @sampleplaces.each do |sampleplace| %>
  <tr>
    <td><%= sampleplace.name %></td>
    <td><%= link_to 'View', sampleplace %></td>
    <td><%= link_to 'Edit', edit_sampleplace_path(sampleplace) %></td>
    <td><%= link_to 'Delete', sampleplace, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<% end %>
<%= link_to 'Back to Sample Places', sampleplaces_path %>
