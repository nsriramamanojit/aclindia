<h1>Search Results</h1>
<% if @states.blank? %>
 	<p>Oops!!! No Matches found.</p>
<% else %>
<table id="rounded-corner">
    <thead>
    	<tr>
        	<th scope="col" class="rounded-company">State Name</th>
            <th scope="col" class="rounded-q1">View</th>
            <th scope="col" class="rounded-q2">Edit</th>
            <th scope="col" class="rounded-q3">Delete</th>
 
        </tr>
    </thead>
         <tfoot>
    	<tr>
        	<td colspan="3" class="rounded-foot-left"></td>
        	<td class="rounded-foot-right">&nbsp;</td>
        </tr>
    </tfoot>
<% @states.each do |state| %>
  <tr>
    <td><%= state.name %></td>
    <td><%= link_to 'View', state %></td>
    <td><%= link_to 'Edit', edit_state_path(state) %></td>
    <td><%= link_to 'Delete', state, :confirm => 'Are you sure?',
       :method => :delete %></td>
  </tr>
<% end %>
</table>
<% end %>
<%= link_to 'Back to States', states_path %>
