<h1>Search Results</h1>
<% if @exchanges.blank? %>
 	<p>Oops!!! No Matches found.</p>
<% else %>
<table id="rounded-corner">
    <thead>
    	<tr>
        	<th scope="col" class="rounded-company">Exchange Name</th>
            <th scope="col" class="rounded-q1">View</th>
            <th scope="col" class="rounded-q2">Edit</th>
            <th scope="col" class="rounded-q3">Delete</th>
 
        </tr>
    </thead>
         <tfoot>
    	<tr>
        	<td colspan="3" class="rounded-foot-left"></td>
        	<td class="rounded-foot-right">&nbsp;</td>
        </tr>
    </tfoot>
<% @exchanges.each do |exchange| %>
  <tr>
    <td><%= exchange.name %></td>
    <td><%= link_to 'View', exchange %></td>
    <td><%= link_to 'Edit', edit_exchange_path(exchange) %></td>
    <td><%= link_to 'Delete', exchange, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<% end %>
<%= link_to 'Back to Exchanges', exchanges_path %>
